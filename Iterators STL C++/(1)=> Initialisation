/******************************************************************************

Iterators
*******************************************************************************/
#include <iostream>

#include<vector>
using namespace std;

int main()
{
    
    vector<int>v={1,2,3,4,5,6};
    
    
    //iterator declaration
    vector<int>::iterator it=v.begin();
    
    //printing of iterator
    for(it=v.begin();it!=v.end();++it){
        cout<<(*it)<<endl;
    }
    

    return 0;
}

// output

// 1
// 2
// 3
// 4
// 5
// 6
