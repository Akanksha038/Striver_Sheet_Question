/******************************************************************************

binary=>decimal
*******************************************************************************/
#include <iostream>
#include<math.h>
using namespace std;


int main()
{
    int n;
    cin>>n;
    int p=n;
    int ans=0;
     int i=0;
    while(n!=0)
    {
        
    int digit=n%10;
    
    n=n/10;
    
    if(digit==1) 
    {
            ans= ans+(pow(2,i));
        }
        
           i++;
    }
    
    std::cout<<"decimal representation  of "<<p<<" is:"<<ans<<endl;

    return 0;
}


// output

1010
decimal representation  of 1010 is:10
