/******************************************************************************
Given Number N .Print it's factorial

print answer modulo M
where M=47

*******************************************************************************/
#include <iostream>
using namespace std;

int main()
{
    long long fact=1;
    int n;
    int M=47;
    
    cin>>n;
    
    for(int i=2;i<=n;i++){
        fact=(fact*i)%M;
        
    }
    cout<<fact;
    return 0;
}

// output

30
8
