// Online C++ compiler to run C++ program online
#include <iostream>
#include <string>
#include <stack>
using namespace std;

int main() 
{
    
    string str="Hello";
    stack<char>st;
    
    
    for(int i=0;i<str.size();i++)
        {
            st.push(str[i]);
        }
        while(!st.empty())
        {
            cout<<st.top()<<" ";
            st.pop();
        }
        cout<<endl;

    return 0;
}

# output
# o l l e H 
