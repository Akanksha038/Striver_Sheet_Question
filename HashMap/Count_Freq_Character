/******************************************************************************

Count occurence of element by using map

*******************************************************************************/
#include <iostream>
#include<unordered_map>
using namespace std;

int main()
{
    string str="Thiruvananthapuram"; 
    unordered_map<char,int>freq;
    for(int i=0;i<str.length();i++){
        char ch=str[i];
        freq[ch]++;
    }
    
    for(auto i:freq){
        cout<<i.first<<"->"<<i.second<<endl;
    }
    
    

    return 0;
}



output:

m->1
p->1
t->1
r->2
v->1
i->1
u->2
h->2
n->2
a->4
T->1
